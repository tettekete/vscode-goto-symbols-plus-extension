{
  "name": "function-list",
  "displayName": "Function List",
  "description": "It provides a feature to list and move items such as functions, methods, classes, and markdown headings.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tettekete.list-functions",
        "title": "list functions",
        "category": "FunctionList"
      }
    ],
    "configuration": 
    [
      {
        "title": "%functionList.config.title.indentation%",
        "properties":{
          "functionList.indentation": {
            "type": "string",
            "enum": [
              "none",
              "indent",
              "tree"
            ],
            "enumItemLabels": [
                    "%functionList.config.indentation.none%",
                    "%functionList.config.indentation.indent%",
                    "%functionList.config.indentation.tree%"
                  ],
            "default": "none",
            "markdownDescription": "%functionList.config.desc.indentation%"
          },
          "functionList.indentString": {
            "type": "string",
            "default": "  ",
            "markdownDescription": "%functionList.config.desc.indentString%"
          }
        }
      },
      {
        "title": "%functionList.config.title.misc%",
        "properties":{
          "functionList.prefixString": {
            "type": "string",
            "default": "",
             "markdownDescription": "%functionList.config.desc.prefixString%"
          },
          "functionList.showSymbolKind": {
            "type": "boolean",
            "default": false,
             "markdownDescription": "%functionList.config.desc.showSymbolKind%"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "rm -rf ./dist ; webpack",
    "watch": "webpack --watch",
    "package": "rm -rf ./dist ; webpack --mode production --devtool hidden-source-map",
    "compile-tests": "rm -rf ./out ; tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "ts-loader": "^9.5.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
